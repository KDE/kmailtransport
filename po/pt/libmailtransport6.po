#, kde-format
msgid "TLS encryption"
msgstr "Encriptação TLS"

#. i18n: ectx: label, entry (authenticationType), group (Transport $(transportId))
#: mailtransport.kcfg:70
#, kde-format
msgid "Authentication method"
msgstr "Método de autenticação"

